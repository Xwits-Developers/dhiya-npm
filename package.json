{
  "name": "dhiya-npm",
  "version": "1.0.3",
  "description": "Production-ready client-side RAG framework for browsers: WebGPU embeddings, vector search, offline store, privacy-first.",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist-cjs/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "dist-cjs",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist dist-cjs",
    "example": "npm run build && cd example && npm install --omit=optional && npx vite",
    "example:install": "cd example && npm install --omit=optional",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "rag",
    "retrieval-augmented-generation",
    "client-side rag",
    "browser rag",
    "offline rag",
    "vector search",
    "webgpu embeddings",
    "wasm embeddings",
    "local llm",
    "privacy rag",
    "in-browser ai",
    "rag framework",
    "llm",
    "langchain rag retriever"
  ],
  "author": "Deep Parmar <deep.parmar@sunbots.in>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xwits-Developers/dhiya-npm.git"
  },
  "bugs": {
    "url": "https://github.com/Xwits-Developers/dhiya-npm/issues"
  },
  "homepage": "https://github.com/Xwits-Developers/dhiya-npm#readme",
  "funding": "https://github.com/sponsors/deepparmar",
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "idb": "^8.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.16.0",
    "fake-indexeddb": "^6.2.3",
    "happy-dom": "^20.0.1",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
